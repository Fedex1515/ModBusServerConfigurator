<Window x:Class="ModBusServerConfigurator.AddProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModBusServerConfigurator"
        mc:Ignorable="d"
        Title="Nuovo Profilo" Height="82" Width="397.5" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Nuovo profilo:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxProfile" HorizontalAlignment="Left" Height="23" Margin="101,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="184" KeyUp="TextBoxProfile_KeyUp"/>
        <Button x:Name="ButtonSave" Content="Salva" HorizontalAlignment="Left" Margin="292,13,0,0" VerticalAlignment="Top" Width="74" Height="23" Click="ButtonSave_Click"/>

    </Grid>
</Window>
